<%= render 'users/user_info', {user: current_user, book: @book} %>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= user.image %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show' , book_path(book.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>